{
    "mode": "local",
    "env": {
        "verbose": false,
        "outdir": "./logdir/acc",
    },
    "dataset": {
        "name": "mag240m-c",
        "root": "/mnt/md0/hb_datasets/mag240m_c",
    },
    "model": {
        "arch": "gat",
        "num_hidden": 256,
        "num_layers": 3,
        "dropout": 0.5,
        "lr": 1e-3,
        "runs": 3,
        "epochs": 30,
        "eval_every": 1,
        "train_runahead": 5,
    },
    "sample": {
        "train": [ {
                "sampler": "cluster",
                "partition": "rand",
                "P": 1024,
                "batch_size": 64,
                "pivots": true,
                "num_workers": 0,
            }, {
                "sampler": "ns",
                "fanout": "15,10,5",
                "batch_size": 1000,
                "num_workers": 12,
                "num_prefetch": 20,
            }
        ],
        "eval": {
            "sampler": "ns",
            "fanout": "20,20,20",
            "batch_size": 300,
            "num_workers": 4,
        },
    }
}
