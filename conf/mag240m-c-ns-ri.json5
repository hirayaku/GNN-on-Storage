{
    "mode": "local",
    "env": {
        "verbose": true,
        "outdir": "./logdir/acc",
    },
    "dataset": {
        "name": "mag240m-c",
        "root": "/mnt/md0/hb_datasets/mag240m_c",
    },
    "model": {
        "arch": "sage-ri",
        "num_hidden": 1024,
        "num_layers": 3,
        "dropout": 0.1,
        "lr": 1e-3,
        "lr_schedule": "plateau",
        "lr_step": 1000,
        "lr_decay": 0.8,
        "runs": 1,
        "epochs": 30,
        "log_every" : 100,
        "eval_every": 1,
        "ckpt": "ns-ri",
        "train_runahead": 5,
    },
    "sample": {
        "train": [{
            "sampler": "ns",
            "fanout": "12,12,12",
            "batch_size": 1024,
            "num_workers": 12,
            "drop_last": true,
        }],
        "eval": {
            "sampler": "ns",
            "fanout": "20,20,20",
            "batch_size": 200,
            "num_workers": 12,
        },
    }
}
