{
    "mode": "local",
    "env": {
        "verbose": false,
        "outdir": "./logdir/acc",
    },
    "dataset": {
        "name": "ogbn-papers100M",
        "root": "/mnt/md0/hb_datasets/ogbn_papers100M",
    },
    "model": {
        "arch": "sage",
        "num_hidden": 256,
        "num_layers": 3,
        "dropout": 0.5,
        "lr": 1e-3,
        "runs": 1,
        "epochs": 25,
        "log_every" : 100,
        "eval_after": 0,
        "eval_every": 1,
    },
    "sample": {
        "train": [ {
                "sampler": "cluster",
                "partition": "rand",
                "P": 64,
                "batch_size": 8,
                "pivots": false,
                "num_repeats": 1,
                "num_workers": 0,
            }, {
                "sampler": "ns",
                "fanout": "15,10,5",
                "batch_size": 1000,
                "num_workers": 12,
                "prefetch_steps": 100,
            }
        ],
        "eval": {
            "sampler": "ns",
            "fanout": "20,20,20",
            "batch_size": 300,
            "num_workers": 8,
        },
    }
}
