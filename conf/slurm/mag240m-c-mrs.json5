{
    "mode": "local",
    "env": {
        "verbose": false,
        "outdir": "./logdir/acc",
    },
    "dataset": {
        "name": "mag240m-c",
        "root": "/data/scratch/tianhaoh/datasets/gnnos/mag240m_c",
    },
    "model": {
        "arch": "gin",
        "num_hidden": 256,
        "num_layers": 3,
        "dropout": 0.5,
        "lr": 1e-3,
        "runs": 3,
        "epochs": 30,
        "ckpt": "marius",
        "train_runahead": 5,
        "eval_every": 1,
        "eval_after": 0,
    },
    "sample": {
        "train": [ {
                "sampler": "cluster",
                "partition": "rand",
                "P": 1024,
                "batch_size": 64,
                "pivots": true,
                "num_repeats": 1,
                "num_workers": 0,
            }, {
                "sampler": "ns",
                "fanout": "15,10,5",
                "batch_size": 1000,
                "num_workers": 20,
                "num_prefetch": 20,
            }
        ],
        "eval": {
            "sampler": "ns",
            "fanout": "15,10,5",
            "batch_size": 1000,
            "num_workers": 16,
        },
    }
}
