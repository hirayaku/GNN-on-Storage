{
    "mode": "local",
    "env": {
        "verbose": true,
        "outdir": "./logdir/acc",
    },
    "dataset": {
        "name": "mag240m-c",
        "root": "/data/scratch/tianhaoh/datasets/gnnos/mag240m_c",
    },
    "model": {
        "arch": "sage-ri",
        "num_hidden": 1024,
        "num_layers": 3,
        "dropout": 0.1,
        "lr": 1e-3,
        "lr_schedule": "plateau",
        "lr_step": 1000,
        "lr_decay": 0.8,
        "runs": 3,
        "epochs": 30,
        "eval_every": 1,
        "eval_test": false,
    },
    "sample": {
        "train": [{
            "sampler": "ns",
            "fanout": "15,10,5",
            "batch_size": 1024,
            "num_workers": 32,
        }],
        "eval": {
            "sampler": "ns",
            "fanout": "20,20,20",
            "batch_size": 100,
            "num_workers": 32,
        },
    }
}
