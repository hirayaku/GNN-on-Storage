{
    "mode": "local",
    "env": {
        "verbose": true,
        "outdir": "./logdir/acc",
    },
    "dataset": {
        "name": "ogbn-papers100M",
        "root": "/data/scratch/tianhaoh/datasets/gnnos/ogbn_papers100M",
    },
    "model": {
        "arch": "sage",
        "num_hidden": 256,
        "num_layers": 3,
        "dropout": 0.5,
        "lr": 1e-3,
        "runs": 3,
        "epochs": 60,
        "eval_every": 1,
        "eval_test": false,
    },
    "sample": {
        "train": [{
            "sampler": "ns",
            "fanout": "15,10,5",
            "batch_size": 4096,
            "num_workers": 32,
        }],
        "eval": {
            "sampler": "ns",
            "fanout": "20,20,20",
            "batch_size": 500,
            "num_workers": 16,
        },
    }
}
